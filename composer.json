{
    "name": "pelso/openapi-validator-bundle",
    "version": "dev-main",
    "description": "OpenAPI validator Bundle for Symfony",
    "type": "symfony-bundle",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "symfony/framework-bundle": "*",
        "league/openapi-psr7-validator": "^0.22",
        "symfony/psr-http-message-bridge": "*",
        "nyholm/psr7": "1.8.x-dev"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/dependency-injection": "^v2.7",
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pelso\\OpenAPIValidatorBundle\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pelso\\OpenAPIValidatorBundle\\Tests\\": "./Tests"
        }
    },
    "authors": [
        {
            "name": "Kornel Balatoni"
        }
    ],
    "minimum-stability": "dev"
}
